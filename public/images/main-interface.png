<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#1a1a2e"/>
  <text x="200" y="150" text-anchor="middle" fill="#a29bfe" font-size="24" font-family="Arial" font-weight="bold">
    3D Visualization
  </text>
  <text x="200" y="180" text-anchor="middle" fill="#fd79a8" font-size="16" font-family="Arial">
    Interactive Point Cloud
  </text>
  <!-- 3D point cloud representation -->
  <circle cx="100" cy="80" r="4" fill="#6c5ce7"/>
  <circle cx="150" cy="120" r="4" fill="#6c5ce7"/>
  <circle cx="200" cy="100" r="4" fill="#6c5ce7"/>
  <circle cx="250" cy="140" r="4" fill="#6c5ce7"/>
  <circle cx="300" cy="90" r="4" fill="#6c5ce7"/>
  <circle cx="120" cy="200" r="4" fill="#6c5ce7"/>
  <circle cx="180" cy="180" r="4" fill="#6c5ce7"/>
  <circle cx="220" cy="220" r="4" fill="#6c5ce7"/>
  <circle cx="280" cy="190" r="4" fill="#6c5ce7"/>
  <circle cx="320" cy="160" r="4" fill="#6c5ce7"/>
  <!-- Connecting lines -->
  <line x1="100" y1="80" x2="150" y2="120" stroke="#a29bfe" stroke-width="1"/>
  <line x1="150" y1="120" x2="200" y2="100" stroke="#a29bfe" stroke-width="1"/>
  <line x1="200" y1="100" x2="250" y2="140" stroke="#a29bfe" stroke-width="1"/>
  <line x1="250" y1="140" x2="300" y2="90" stroke="#a29bfe" stroke-width="1"/>
  <line x1="120" y1="200" x2="180" y2="180" stroke="#a29bfe" stroke-width="1"/>
  <line x1="180" y1="180" x2="220" y2="220" stroke="#a29bfe" stroke-width="1"/>
  <line x1="220" y1="220" x2="280" y2="190" stroke="#a29bfe" stroke-width="1"/>
</svg>